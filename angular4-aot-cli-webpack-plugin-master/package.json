{
  "name": "angular4-aot-example",
  "version": "1.0.0",
  "description": "Angular 4 AOT (Ahead Of Time) compilation with the Angular CLI Webpack plugin",
  "main": "build/modules/main.module.js",
  "scripts": {
    "cleanup": "rimraf ../Demo02/wwwroot",
    "start": "webpack-dev-server --config webpack.aot.config.js",
    "build": "npm run cleanup && webpack --config webpack.aot.config.js -p",
    "jit": "webpack-dev-server --config webpack.jit.config.js",
    "aot": "webpack-dev-server --config webpack.aot.config.js"
  },
  "keywords": [
    "angular4",
    "aot",
    "typescript"
  ],
 
  "license": "ISC",
  "dependencies": {
    "@angular/common": "4.0.0",
    "@angular/compiler": "4.0.0",
    "@angular/core": "4.0.0",
    "@angular/http": "4.0.0",
    "@angular/animations":"4.0.0",
    "@angular/platform-browser": "4.0.0",
    "@angular/platform-browser-dynamic": "4.0.0",
    "@angular/router": "4.0.0",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "zone.js": "0.8.5"
  },
  "devDependencies": {
    "@angular/compiler-cli": "4.0.0",
    "@angular/platform-server": "4.0.0",
    "@ngtools/webpack": "1.3.0",
    "@types/node": "7.0.11",
    "@ultimate/aot-loader": "0.1.15",
    "angular2-template-loader": "0.6.2",
    "apply-loader": "1.0.0",
    "awesome-typescript-loader": "3.1.2",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.26.1",
    "extract-text-webpack-plugin": "2.0.0-rc.3",
    "gh-pages": "0.12.0",
    "http-server": "0.9.0",
    "node-sass": "4.5.0",
    "sass-loader": "4.1.1",
    "pug": "2.0.0-beta6",
    "pug-loader": "2.3.0",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.0",
    "sass-loader": "6.0.2",
    "style-loader": "0.13.1",
    "to-string-loader": "1.1.5",
    "typescript": "2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1"
  }
